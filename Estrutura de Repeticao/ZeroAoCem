using System;
					
public class ZeroAoCem
{
	public static void Main()
	{
		int cont2 = 1, cont3 = 1;
		for(int cont = 1 ; cont <= 100 ; cont++){
		Console.WriteLine(cont);
	}

	while(cont2 <= 100)	{
		Console.WriteLine(cont2);
		cont2++;
	}
		
	do	{
		Console.WriteLine(cont3);
		cont3++;
		} while(cont3 <= 100);
	}
}
